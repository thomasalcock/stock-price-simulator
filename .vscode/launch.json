{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "n_paths=5",
        "drift=0.06",
        "volatility=0.04",
        "initial_value=50",
        "delta_t=0.005",
        "total_time=1.5",
        "process_type=brownian",
        "output_file_name=results.csv"
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/thomas/projects/cpp_stuff/stats",
      "program": "/home/thomas/projects/cpp_stuff/stats/simulator-debug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build-debug"
    }
  ]
}